{
  "java.referencesCodeLens.enabled": false,
  "workbench.editor.enablePreview": false,
  "editor.multiCursorModifier": "ctrlCmd",
  "workbench.statusBar.visible": true,
  "window.menuBarVisibility": "toggle",
  "workbench.colorTheme": "Dracula",
  "explorer.openEditors.visible": 0,
  "editor.fontFamily": "JetBrains Mono",
  "editor.suggestSelection": "first",
  "editor.fontLigatures": true,
  "editor.selectionClipboard": false,
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "editor.minimap.enabled": false,
  "terminal.external.linuxExec": "tilix",
  "terminal.integrated.sendKeybindingsToShell": true,
  "editor.rulers": [80,120],
  "[xml]": {},
  "workbench.iconTheme": "material-icon-theme",
  "mesonbuild.configureOnOpen": false,
  "shellcheck.run": "onSave",
  "workbench.startupEditor": "newUntitledFile",
  "files.trimTrailingWhitespace": true,
  "python.linting.enabled": true,
  "python.linting.flake8Enabled": true,
  "python.linting.flake8Args": [
    "--max-line-length=120",
    "--ignore=F841"
  ],
  "python.linting.pylintEnabled": false,
  "files.associations": {
    "Containerfile": "dockerfile",
    "*.xls": "excel",
    "Justfile": "makefile"
  },
  "telemetry.telemetryLevel": "off",
  "java.debug.settings.enableRunDebugCodeLens": false,
  "codesnap.backgroundColor": "#282a36",
  "codesnap.showWindowControls": false,
  "codesnap.containerPadding": "1em",
  "mesonbuild.buildFolder": "build",
  "d.servedPath": "/home/tryton/.local/share/code-d/bin/serve-d",
  "dscanner.ignoredKeys": [
    "dscanner.style.undocumented_declaration"
  ],
  "go.useLanguageServer": true,
  "codesnap.shutterAction": "save",
  "go.toolsManagement.autoUpdate": false,
  "window.titleBarStyle": "custom",
  "redhat.telemetry.enabled": false,
  "security.workspace.trust.untrustedFiles": "open",
  "editor.codeLens": false,
  "explorer.confirmDelete": false
}
